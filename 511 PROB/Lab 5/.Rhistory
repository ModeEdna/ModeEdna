(pm = matrix(c(.2,0,.2,0,0,0,.2,.2,0,.25,0,0,.2,.5,.5,.8,.8,.2,.5,0,0,0,.2,0,.25), ncol = 5))
# function to compute matrix powers, using the same one as the diary
powerP = function(A,n){
B <- 0*A
diag(B) <- 1
for (j in 1:n){
B <- B%*%A
}
return(B)
}
powerP(pm,13)[3,4]
