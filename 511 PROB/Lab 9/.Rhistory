alpha.fn <- function(data, index) {
X <- data$X[index]
Y <- data$Y[index]
(var(Y) - cov(X, Y)) / (var(X) + var(Y) - 2 * cov(X, Y))
}
boot.out= boot(df[df['Carrier']=='UA'],alpha.fn,R=1000) #repeat this 1000 times
